generator client {
  provider = "prisma-client"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
}

enum AuthProvider {
  GOOGLE
  NAVER
}

model User {
  id           String       @id @default(cuid())
  email        String       @unique
  name         String?
  profileImage String?
  provider     AuthProvider
  providerId   String
  createdAt    DateTime     @default(now())
  updatedAt    DateTime     @updatedAt

  @@unique([provider, providerId])
}
